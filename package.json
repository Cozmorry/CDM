{
  "name": "cozy-download-manager",
  "version": "1.0.0",
  "description": "A cozy and modern download manager built with Electron",
  "main": "src/main.js",
    "scripts": {
      "start": "electron .",
      "dev": "electron . --dev",
      "build": "electron-builder"
    },
  "keywords": [
    "download-manager",
    "electron",
    "cozy"
  ],
  "author": "Cozmorry",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "axios": "^1.6.2"
  },
  "build": {
    "appId": "com.cozy.downloadmanager",
    "productName": "Cozy Download Manager",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "assets/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  }
}
